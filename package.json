{
  "name": "groups-messaging",
  "version": "1.0.0",
  "description": "groups messaging web test automation",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "cy:run": "cypress run --browser chrome",
    "report:merge": "mochawesome-merge cypress/results/*.json -o cypress/report.json",
    "report:generate": "marge cypress/report.json --charts true"
  },
  "author": "Iesha Osias",
  "license": "ISC",
  "devDependencies": {
    "@badeball/cypress-cucumber-preprocessor": "^20.0.2",
    "@cypress/browserify-preprocessor": "^3.0.2",
    "cypress": "^13.6.6",
    "mocha": "^10.3.0",
    "mochawesome": "^7.1.3",
    "mochawesome-merge": "^4.3.0",
    "mochawesome-report-generator": "^6.2.0",
    "typescript": "^5.4.2"
  },
  "cypress-cucumber-preprocessor": {
    "nonGlobalStepDefinitions": true,
    "stepDefinitions": [
      "cypress/e2e/[filepath]/**/*.{js,ts}",
      "cypress/e2e/[filepath].{js,ts}",
      "cypress/support/step_definitions/**/*.{js,ts}"
    ]
  },
  "dependencies": {
    "cypress-mailosaur": "^2.14.0",
    "dotenv": "^16.4.5",
    "ts-node": "^10.9.2"
  }
